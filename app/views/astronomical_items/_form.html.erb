<%= form_for @astronomical_item do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :category %>
    <%= f.text_field :category %>

    <%= f.label :summary %>
    <%= f.text_area :summary %>

    <%= f.label :image %>
    <%= f.url_field :image %>

    <%= f.submit %>

<% end %>

